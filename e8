@echo off
echo Detecting Viruses......
:start
cd C:\
IF EXIST swing.bat goto infected
IF NOT EXIST virus.bat goto clean
:infected
echo WARNING SWING.BAT VIRUS DETECTED!
del swing.bat
pause
goto start
:clean
echo your System is secure now and virus has been deleted!
pause
exit


%O|%O
